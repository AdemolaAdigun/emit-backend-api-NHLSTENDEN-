{
  "name": "emit-it",
  "version": "1.0.0",
  "description": "api and logic of emit-it system",
  "main": "server.js",
  "scripts": {
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel -d ./build ./src --source-maps",
    "clean": "rm -rf .nyc_output build coverage && mkdir build",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo:all",
    "db:reset": "npm run db:migrate:undo && npm run db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:reseed": "npm run db:migrate:undo && npm run db:migrate && sequelize db:seed:all",
    "dev": "cross-env DEBUG=dev nodemon --exec babel-node src/server.js",
    "heroku-prebuild": "export NPM_CONFIG_PRODUCTION=false; export NODE_MODULES_CACHE=false; export NODE_ENV=development; npm install",
    "heroku-postbuild": "export NPM_CONFIG_PRODUCTION=true; export NODE_ENV=production;",
    "lint": "eslint \"**/*.js\" --fix",
    "postinstall": "npm run build && npm run db:migrate",
    "pretest": "cross-env NODE_ENV=test npm run db:migrate:undo && cross-env NODE_ENV=test npm run db:migrate",
    "posttest": "cross-env NODE_ENV=test npm run db:migrate:undo",
    "start": "node ./build/server.js",
    "sync-env": "sync-dotenv"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Emit-IT-NHL-Stenden/backend.git"
  },
  "author": "Ademola Adigun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Emit-IT-NHL-Stenden/backend/issues"
  },
  "homepage": "https://github.com/Emit-IT-NHL-Stenden/backend#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "babel-preset-env": "^1.7.0",
    "husky": "^6.0.0",
    "install": "^0.13.0",
    "jsonschema": "^1.4.0",
    "nodemon": "^2.0.7",
    "npm": "^7.9.0"
  },
  "engines": {
    "node": "12.18.3",
    "npm": "6.14.9"
  },
  "dependencies": {
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-regenerator": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/register": "^7.13.14",
    "@babel/runtime": "^7.13.10",
    "axios": "^0.21.1",
    "babel-core": "^6.26.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "express-validator": "^6.10.0",
    "faker": "^4.1.0",
    "helmet": "^4.6.0",
    "http": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mocha": "^7.0.0",
    "multer-storage-cloudinary": "^2.2.1",
    "pg": "^8.6.0",
    "q": "^1.5.1",
    "regenerator-runtime": "^0.13.7",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "uuid": "^8.3.2"
  }
}
